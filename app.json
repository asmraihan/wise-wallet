{
  "expo": {
    "name": "Wise Wallet",
    "slug": "wise-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wallet_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wallet_icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.asmraihan.wisewallet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fbabce1f-5ae3-4e76-9f53-9aaa6d523987"
      }
    },
    "owner": "asmraihan"
  }
}
